<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<catalog xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:noNamespaceSchemaLocation="CatalogSchema.xsd">

    <effectiveDate>2021-02-01T00:00:00+00:00</effectiveDate>
    <catalogName>ClearFlask</catalogName>
    <recurringBillingMode>IN_ADVANCE</recurringBillingMode>

    <currencies>
        <currency>USD</currency>
    </currencies>

    <units>
        <!-- If changed, also change unit name in KillBilling.java -->
        <unit prettyName="Active Users" name="active-user"/>
    </units>

    <products>
        <product prettyName="Growth" name="growth">
            <category>BASE</category>
        </product>
        <product prettyName="Standard" name="standard">
            <category>BASE</category>
        </product>
        <product prettyName="Flat" name="flat">
            <category>BASE</category>
        </product>
    </products>

    <rules>
        <changePolicy>
            <changePolicyCase>
                <phaseType>TRIAL</phaseType>
                <policy>IMMEDIATE</policy>
            </changePolicyCase>
            <changePolicyCase>
                <fromProduct>growth</fromProduct>
                <toProduct>standard</toProduct>
                <policy>IMMEDIATE</policy>
            </changePolicyCase>
            <changePolicyCase>
                <toProduct>flat</toProduct>
                <policy>IMMEDIATE</policy>
            </changePolicyCase>
            <changePolicyCase>
                <policy>END_OF_TERM</policy>
            </changePolicyCase>
        </changePolicy>
        <changeAlignment>
            <changeAlignmentCase>
                <toProductCategory>ADD_ON</toProductCategory>
                <alignment>START_OF_BUNDLE</alignment>
            </changeAlignmentCase>
            <changeAlignmentCase>
                <alignment>START_OF_SUBSCRIPTION</alignment>
            </changeAlignmentCase>
        </changeAlignment>
        <cancelPolicy>
            <cancelPolicyCase>
                <policy>END_OF_TERM</policy>
            </cancelPolicyCase>
        </cancelPolicy>
        <createAlignment>
            <createAlignmentCase>
                <alignment>START_OF_BUNDLE</alignment>
            </createAlignmentCase>
        </createAlignment>
        <billingAlignment>
            <billingAlignmentCase>
                <alignment>SUBSCRIPTION</alignment>
            </billingAlignmentCase>
        </billingAlignment>
        <priceList>
            <priceListCase>
                <toPriceList>DEFAULT</toPriceList>
            </priceListCase>
        </priceList>
    </rules>

    <plans>
        <plan name="growth2-monthly" prettyName="Growth Plan">
            <product>growth</product>
            <initialPhases>
                <phase type="TRIAL" prettyName="Trial">
                    <duration>
                        <unit>DAYS</unit>
                        <number>14</number>
                    </duration>
                    <fixed>
                        <fixedPrice>
                            <price>
                                <currency>USD</currency>
                                <value>0</value>
                            </price>
                        </fixedPrice>
                    </fixed>
                </phase>
            </initialPhases>
            <finalPhase type="EVERGREEN" prettyName="Monthly Billing">
                <duration>
                    <unit>UNLIMITED</unit>
                </duration>
                <recurring>
                    <billingPeriod>MONTHLY</billingPeriod>
                    <recurringPrice>
                        <price>
                            <currency>USD</currency>
                            <value>50.00</value>
                        </price>
                    </recurringPrice>
                </recurring>
                <usages>
                    <usage prettyName="Active Users" name="growth2-active-users" billingMode="IN_ARREAR"
                           usageType="CONSUMABLE" tierBlockPolicy="ALL_TIERS">
                        <billingPeriod>MONTHLY</billingPeriod>
                        <tiers>
                            <tier>
                                <blocks>
                                    <tieredBlock>
                                        <unit>active-user</unit>
                                        <size>50</size>
                                        <prices>
                                            <price>
                                                <currency>USD</currency>
                                                <value>0</value>
                                            </price>
                                        </prices>
                                        <max>1</max>
                                    </tieredBlock>
                                </blocks>
                            </tier>
                            <tier>
                                <blocks>
                                    <tieredBlock>
                                        <unit>active-user</unit>
                                        <size>50</size>
                                        <prices>
                                            <price>
                                                <currency>USD</currency>
                                                <value>30</value>
                                            </price>
                                        </prices>
                                        <max>-1</max>
                                    </tieredBlock>
                                </blocks>
                            </tier>
                        </tiers>
                    </usage>
                </usages>
            </finalPhase>
        </plan>
        <plan name="standard2-monthly" prettyName="Standard Plan">
            <product>standard</product>
            <initialPhases>
                <phase type="TRIAL" prettyName="Trial">
                    <duration>
                        <unit>DAYS</unit>
                        <number>14</number>
                    </duration>
                    <fixed>
                        <fixedPrice>
                            <price>
                                <currency>USD</currency>
                                <value>0</value>
                            </price>
                        </fixedPrice>
                    </fixed>
                </phase>
            </initialPhases>
            <finalPhase type="EVERGREEN" prettyName="Monthly Billing">
                <duration>
                    <unit>UNLIMITED</unit>
                </duration>
                <recurring>
                    <billingPeriod>MONTHLY</billingPeriod>
                    <recurringPrice>
                        <price>
                            <currency>USD</currency>
                            <value>200.00</value>
                        </price>
                    </recurringPrice>
                </recurring>
                <usages>
                    <usage prettyName="Active Users" name="standard2-active-users" billingMode="IN_ARREAR"
                           usageType="CONSUMABLE" tierBlockPolicy="ALL_TIERS">
                        <billingPeriod>MONTHLY</billingPeriod>
                        <tiers>
                            <tier>
                                <blocks>
                                    <tieredBlock>
                                        <unit>active-user</unit>
                                        <size>300</size>
                                        <prices>
                                            <price>
                                                <currency>USD</currency>
                                                <value>0</value>
                                            </price>
                                        </prices>
                                        <max>1</max>
                                    </tieredBlock>
                                </blocks>
                            </tier>
                            <tier>
                                <blocks>
                                    <tieredBlock>
                                        <unit>active-user</unit>
                                        <size>300</size>
                                        <prices>
                                            <price>
                                                <currency>USD</currency>
                                                <value>100</value>
                                            </price>
                                        </prices>
                                        <max>-1</max>
                                    </tieredBlock>
                                </blocks>
                            </tier>
                        </tiers>
                    </usage>
                </usages>
            </finalPhase>
        </plan>
        <plan name="flat-yearly" prettyName="Flat Plan">
            <product>flat</product>
            <initialPhases>
                <phase type="TRIAL" prettyName="Trial">
                    <duration>
                        <unit>DAYS</unit>
                        <number>14</number>
                    </duration>
                    <fixed>
                        <fixedPrice>
                            <price>
                                <currency>USD</currency>
                                <value>0</value>
                            </price>
                        </fixedPrice>
                    </fixed>
                </phase>
            </initialPhases>
            <finalPhase type="EVERGREEN" prettyName="Annual Billing">
                <duration>
                    <unit>UNLIMITED</unit>
                </duration>
                <recurring>
                    <billingPeriod>ANNUAL</billingPeriod>
                    <recurringPrice>
                        <price>
                            <currency>USD</currency>
                            <value>2000.00</value>
                        </price>
                    </recurringPrice>
                </recurring>
            </finalPhase>
        </plan>
        <!-- Remove below plans in next catalog -->
        <plan name="growth-monthly" prettyName="Growth Plan (Grandfathered)">
            <product>growth</product>
            <initialPhases>
                <phase type="TRIAL" prettyName="Trial">
                    <duration>
                        <!-- Trial is terminated based on PlanStore.java:STOP_TRIAL_AFTER_ACTIVE_USERS_REACHES -->
                        <unit>MONTHS</unit>
                        <number>6</number>
                    </duration>
                    <fixed>
                        <fixedPrice>
                            <price>
                                <currency>USD</currency>
                                <value>0</value>
                            </price>
                        </fixedPrice>
                    </fixed>
                </phase>
            </initialPhases>
            <finalPhase type="EVERGREEN" prettyName="Monthly Billing">
                <duration>
                    <unit>UNLIMITED</unit>
                </duration>
                <recurring>
                    <billingPeriod>MONTHLY</billingPeriod>
                    <recurringPrice>
                        <price>
                            <currency>USD</currency>
                            <value>50.00</value>
                        </price>
                    </recurringPrice>
                </recurring>
                <usages>
                    <usage prettyName="Active Users" name="growth-active-users" billingMode="IN_ARREAR"
                           usageType="CONSUMABLE" tierBlockPolicy="ALL_TIERS">
                        <billingPeriod>MONTHLY</billingPeriod>
                        <tiers>
                            <tier>
                                <blocks>
                                    <tieredBlock>
                                        <unit>active-user</unit>
                                        <size>50</size>
                                        <prices>
                                            <price>
                                                <currency>USD</currency>
                                                <value>0</value>
                                            </price>
                                        </prices>
                                        <max>1</max>
                                    </tieredBlock>
                                </blocks>
                            </tier>
                            <tier>
                                <blocks>
                                    <tieredBlock>
                                        <unit>active-user</unit>
                                        <size>50</size>
                                        <prices>
                                            <price>
                                                <currency>USD</currency>
                                                <value>30</value>
                                            </price>
                                        </prices>
                                        <max>-1</max>
                                    </tieredBlock>
                                </blocks>
                            </tier>
                        </tiers>
                    </usage>
                </usages>
            </finalPhase>
        </plan>
        <plan name="standard-monthly" prettyName="Standard Plan (Grandfathered)">
            <product>standard</product>
            <initialPhases>
                <phase type="TRIAL" prettyName="Trial">
                    <duration>
                        <!-- Trial is terminated based on PlanStore.java:STOP_TRIAL_AFTER_ACTIVE_USERS_REACHES -->
                        <unit>MONTHS</unit>
                        <number>6</number>
                    </duration>
                    <fixed>
                        <fixedPrice>
                            <price>
                                <currency>USD</currency>
                                <value>0</value>
                            </price>
                        </fixedPrice>
                    </fixed>
                </phase>
            </initialPhases>
            <finalPhase type="EVERGREEN" prettyName="Monthly Billing">
                <duration>
                    <unit>UNLIMITED</unit>
                </duration>
                <recurring>
                    <billingPeriod>MONTHLY</billingPeriod>
                    <recurringPrice>
                        <price>
                            <currency>USD</currency>
                            <value>200.00</value>
                        </price>
                    </recurringPrice>
                </recurring>
                <usages>
                    <usage prettyName="Active Users" name="standard-active-users" billingMode="IN_ARREAR"
                           usageType="CONSUMABLE" tierBlockPolicy="ALL_TIERS">
                        <billingPeriod>MONTHLY</billingPeriod>
                        <tiers>
                            <tier>
                                <blocks>
                                    <tieredBlock>
                                        <unit>active-user</unit>
                                        <size>300</size>
                                        <prices>
                                            <price>
                                                <currency>USD</currency>
                                                <value>0</value>
                                            </price>
                                        </prices>
                                        <max>1</max>
                                    </tieredBlock>
                                </blocks>
                            </tier>
                            <tier>
                                <blocks>
                                    <tieredBlock>
                                        <unit>active-user</unit>
                                        <size>300</size>
                                        <prices>
                                            <price>
                                                <currency>USD</currency>
                                                <value>100</value>
                                            </price>
                                        </prices>
                                        <max>-1</max>
                                    </tieredBlock>
                                </blocks>
                            </tier>
                        </tiers>
                    </usage>
                </usages>
            </finalPhase>
        </plan>
    </plans>

    <priceLists>
        <defaultPriceList name="DEFAULT" prettyName="Default">
            <plans>
                <plan>growth-monthly</plan>
                <plan>growth2-monthly</plan>
                <plan>standard-monthly</plan>
                <plan>standard2-monthly</plan>
                <plan>flat-yearly</plan>
            </plans>
        </defaultPriceList>
    </priceLists>

</catalog>
